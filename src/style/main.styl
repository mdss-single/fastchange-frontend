/*  <fonts>  */
@font-face
	font-family 'Sregular'
	src url('../fonts/SFUIDisplay-Regular.otf') format('opentype'),
		url('../fonts/SFUIDisplay-Regular.woff') format('woff');
	font-weight normal
	font-style normal
@font-face
	font-family 'Smedium'
	src url('../fonts/SFUIDisplay-Medium.otf')  format('opentype'),
		url('../fonts/SFUIDisplay-Medium.woff') format('woff');
	font-weight normal
	font-style normal
@font-face
	font-family 'Sbold'
	src url('../fonts/SFUIDisplay-Bold.otf') format('opentype'),
		url('../fonts/SFUIDisplay-Bold.woff') format('woff');
	font-weight normal
	font-style normal;
/*  <fonts>  */

/*  <vars>  */
$bdrs = 5px;
$bgc = #fafafa;
/*  </vars>  */

/*  <general>  */
body
	display -webkit-flex
	display -moz-flex
	display -ms-flex
	display -o-flex
	display flex
	flex-direction column
	color #535353
	font normal 1.5rem/1.3 Sregular, sans-serif;
	background-color #fff
	/*font-size-adjust .5*/
:-moz-placeholder
	opacity 1
	color currentColor
:-ms-input-placeholder
	opacity: 1
	color currentColor
a
	color inherit
	font inherit
	text-decoration none
	transition all .2s
	&:hover
		text-decoration underline
.main
	flex 1
	background-color $bgc
	padding-bottom 65px
/*  </general>  */

/*  <header>  */
.header
	flex-shrink 0
	height 188px
	background-color #1364af
.header__rates
	height 44px
	background-color #10599d
.header-rates__wrap
	display -webkit-flex
	display -moz-flex
	display -ms-flex
	display -o-flex
	display flex
	justify-content space-between
	align-items center
	height 44px
	color #87bff4
.header__logo
	position absolute
	top 46px
	left 15px
	color #fff
	font-size 3.5rem
	font-family: Sbold
	&:hover
		text-decoration none
.header__logo-name
	vertical-align middle
	line-height 1
	margin-left 5px
.header__logo-zone
	vertical-align 8px
	color #8bbae6
	font-size 1.5rem
.header__phone
	position absolute
	top 44px
	left 380px
	color #fff
	&:before
		position absolute
		top 4px
		left -25px
		width 15px
		height 15px
		background-image url("data:image/png;base64,")
		content ''
.header__phone-number
	font-size 1.7rem
	font-family Smedium
.header__phone-descr
	font-size 1.4rem
	margin-top 8px
.header__contacts
	position absolute
	top 40px
	left 670px
	color #fff
	font-size 1.7rem
	font-family Smedium
.header-contacts__jabber
	position relative
	margin 0 0 10px
	&:before
		position absolute
		top 50%
		left -26px
		width 13px
		height 19px
		background-image url("data:image/png;base64,")				
		content ''
		transform translateY(-50%)
.header-contacts__tg
	position relative
	margin 0
	&:before
		position absolute
		top 50%
		left -30px
		width 18px
		height 18px
		background-image url("data:image/png;base64,")
		content ''
		transform translateY(-50%)
.header__cabinet-link
	position absolute
	top 42px
	right 15px
	color #fff
	font-size 1.7rem
	font-family Smedium
	line-height 46px
	background #2f78bb url("../i/icons/icon-header-login.png") no-repeat 26px center
	border-radius $bdrs
	padding 0 32px 0 60px
	&:hover
		text-decoration none
		background-color darken(#2f78bb, 3)
/*  </header>  */

/*  <footer>  */
.footer
	flex-shrink 0
	height 80px
	color #000
	font-size 1.6rem
	font-family 'Open Sans'
	background-color $bgc
.footer__wrap
	display -webkit-flex
	display -moz-flex
	display -ms-flex
	display -o-flex
	display flex
	justify-content space-between
	align-items center
	height 80px
.footer__logo
	margin-right 18px
.footer__nav
	flex:1
	text-align right
.footer-nav__item
	margin-left 45px
/*  </footer>  */

/*  <change>  */
.change
	position relative
	&:before
		position absolute
		top 0
		right 0
		left 0
		z-index 0
		height 158px
		background-color #1364af
		content ''
.change-cols
	display -webkit-flex
	display -moz-flex
	display -ms-flex
	display -o-flex
	display flex
	position relative
.change-col
	width 362px
	background-color #fff
	border-radius $bdrs
	padding 25px 30px 44px
	box-shadow 0 1px 4px rgba(0, 0, 0, .2)
	&:nth-child(2)
		flex:1
		margin 0 10px
	&:nth-child(3)
		width 330px
.change__title
	color #0d4c86
	font-size 3.5rem
	text-align center
	margin 0 0 30px
.change__title--give
	background url("data:image/png;base64,") no-repeat 7px center
.change__title--get
	background url("data:image/png;base64,") no-repeat 5px center
.change__section-title
	position relative
	overflow hidden
	color #535353
	font-size 1.6rem
	white-space nowrap
	text-align center
	margin-bottom 12px
	&:before
		position absolute
		top 50%
		right 0
		left 0
		height 1px
		background-color #ececec
		content ''
	& span
		position relative
		background-color #fff
		padding 0 22px
.change-section__title-link
	position absolute
	top 0
	right 0
	color #5889c3
	text-decoration underline
	background-color #fff
	padding-left 7px
	&:hover
		text-decoration none
.change__section-title--get
	display -webkit-flex
	display -moz-flex
	display -ms-flex
	display -o-flex
	display flex
	justify-content space-between
	margin 0 7px 12px
	& span
		padding 0
	&:before
		display none
.change-col--data .change__section-title
	color #000
.change-currency
	display -webkit-flex
	display -moz-flex
	display -ms-flex
	display -o-flex
	display flex
	justify-content center
	color #000
	font-size 1.6rem
	margin 25px 0 40px
.change-curerency__radio
	position absolute
	left -9999px
	& + label
		position relative
		line-height 1
		padding-left 20px
		margin 0 12px
		cursor pointer
		&:before
			position absolute
			top 3px
			left 0
			width 12px
			height 12px
			background-color #dfdfdf
			border-radius 50%
			content ''
			transition all .2s
	&:checked + label
		cursor default
		&:before
			background-color #0888ff
.change-system
	overflow-y auto
	max-height 584px
	margin 0 -10px
.change-system__row
	display -webkit-flex
	display -moz-flex
	display -ms-flex
	display -o-flex
	display flex
	position relative
	color #000
	font-size 1.6rem
	line-height 1
	border-radius $bdrs
	padding 9px 17px 9px 42px
	margin 7px 0
	cursor pointer
	transition all .1s
	&:hover
		color #fff
		background-color #0888ff
		.change-system__balance
			color #fff
.change-system__row--active
	color #fff
	background-color #0888ff
	cursor default
	.change-system__balance
		color #fff
.change-col--get .change-system__row
	padding 9px 17px 9px 50px
.change-system__icon
	position absolute
	top 50%
	left 21px
	margin-right 12px
	transform translate(-50%, -50%)
.change-col--get .change-system__icon
	left 25px
.change-system__name
	flex 1
	margin 0 15px 0 24px
.change-system__balance
	color #ababab
.change-system__notice
	display none
	position absolute
	top 48px
	left 0
	z-index 15
	width 100%
	color #fff
	font-family Sbold
	background-color rgba(81, 89, 103, .95)
	border-radius 5px
	padding 15px
	&:before
		position absolute
		top -6px
		left 50%
		border solid transparent
		border-width 0 6px 6px
		border-bottom-color rgba(81, 89, 103, .95)
		content ''
		transform translateX(-50%)
[data-notice]:hover
	.change-system__notice
		display block
		animation fade .5s
.change-system__notice-title
	font-size 1.3rem
	margin 0 0 8px
.change-system__notice-descr
	font-size 1.4rem
	line-height 1.8rem
	p
		margin 0
.change__section
	position relative
	margin 5px 0 20px
.change__section-input
	padding 0 12px 0 42px
.change__section-pic
	position absolute
	top 50%
	left 21px
	transform translate(-50%, -50%)
.change__get
	position relative
	margin-top 60px
	&:before
		position absolute
		top -20px
		right 0
		left 0
		height 1px
		background-color #e3e9f0
		content ''
	&:after
		position absolute
		top -24px
		left 50%
		width 9px
		height @width
		background-color #fff
		border solid #e3e9f0
		border-width 0 1px 1px 0
		content ''
		transform translateX(-50%) rotate(45deg)
.change__total
	text-align center
	margin 35px 0 25px	
/*  </change>  */

/*  <refill-request>  */
.refill-request
	width 672px
	padding 80px 130px 85px
.refill-request__title
	color #000
	font-size 3rem
	text-align center
	margin 0 -20px 30px
.refill-request__descr
	font-size 1.7rem
	line-height 2.4rem
	text-align center
	margin 0 -36px 20px
.refill-request__row
	margin-bottom 20px
.refill-request__row-title
	display block
	font-size 1.6rem
	margin-bottom 10px
.refill-request__input
	height 47px
.refill-request__btn
	margin-top 38px
/*  </refill-request>  */

/*  <exchange-finish>  */
.exchange-finish
	width 672px
	padding 70px 152px 85px 98px
	counter-reset:exchange
.exchange-finish__title
	color #000
	font-size 3rem
	line-height 3.6rem
	margin 0 0 50px
.exchange-finish__row
	position relative
	color #000
	font-size 1.7rem
	margin 0 0 30px 58px
	&:before
		position absolute
		top -15px
		left -50px
		color #0e7be1
		font-size 4rem
		counter-increment:exchange
		content counter(exchange)"."
.exchange-finish__details
	width 100%
	margin-top 25px
	td
		vertical-align top
		background-color #f1f1f1
		padding 15px 0
		&:first-child
			color #535353
			padding 15px 20px 15px 26px
		&:last-child
			font-family Smedium
			word-break break-all
			padding 15px 26px 15px 0
.exchange-finish__btns
	display -webkit-flex
	display -moz-flex
	display -ms-flex
	display -o-flex
	display flex
	justify-content flex-end
	align-items center
	.btn
		padding 0 50px
.exchange-finish__cancel
	color #000
	font-size 2rem
	margin-left 50px
/*  </exchange-finish>  */

/*  <request-complete>  */
.request-complete
	width 672px
	background url('../i/request-complete__bg.png') no-repeat center 36px
	padding 165px 65px 55px
.request-complete__title
	color #000
	font-size 3rem
	text-align center
	margin 0 0 30px
.request-complete__row
	font-size 1.7rem
	line-height 2.4rem
	text-align center
	margin-bottom 30px
	p
		margin 0
.request-complete__btn
	display block
	padding 0 110px
	margin 60px auto 0
/*  </request-complete>  */

/*  <error-page>  */
.error-page
	text-align center
	margin-top 135px
.error-page__title
	color #000
	font-size 3.6rem
	margin 0 0 30px
.error-page__descr
	max-width 770px
	font-size 1.7rem
	line-height 2.4rem
	margin 0 auto 80px
	p
		margin 0
.error-page__btn
	padding 0 64px
/*  </error-page>  */

/*  <contacts>  */
.contacts
	display -webkit-flex
	display -moz-flex
	display -ms-flex
	display -o-flex
	display flex
	margin-top 60px
.contacts__main
	flex 1
	margin-right 215px
.contacts__section
	margin-bottom 55px
	&:last-child
		margin-bottom 0
.contacts-section__title
	color #000
	font-size 3rem
	margin 0 0 10px
.contacts-section__sub
	font-size 1.7rem
	line-height 1
	margin-bottom 24px
	&:last-child
		margin-bottom 0
	p
		margin 0
	a
		color #6298d8
		text-decoration underline
		&:hover
			text-decoration none
.contacts-section__sub--reviews a
	margin-right 25px
.contacts__sidebar
	width 228px
.contacts__aside-section
	margin-bottom 50px
	&:last-child
		margin-bottom 0
.contacts-aside__title
	font-size 1.7rem
	margin 0 0 12px
.contacts__soc
	display -webkit-flex
	display -moz-flex
	display -ms-flex
	display -o-flex
	display flex
	justify-content space-between
.contacts__soc-item
	width 44px
	height @width
	background-color #fff
	background-repeat no-repeat
	background-position center center
	border 2px solid #a1bed9
	border-radius 50%
	&:hover
		border-color darken(#a1bed9, 20)
.contacts__soc-item--vk
	background-image url("data:image/png;base64,")
.contacts__soc-item--twitter
	background-image url("data:image/png;base64,")
	background-position 10px center
.contacts__soc-item--fb
	background-image url("data:image/png;base64,")
.contacts__soc-item--tg
	background-image url("data:image/png;base64,")
	background-position 7px center
/*  </contacts>  */

/*  <faq>  */
.faq
	overflow hidden
	margin-top 65px
.faq__title
	color #000
	font-size 3rem
	margin 0 0 40px
.faq__nav
	position relative
	margin-bottom 46px
	&:before
		position absolute
		right -10000%
		bottom -11px
		left -10000%
		height 2px
		background-color #e8e8e8
		content ''
.faq-nav__item
	position relative
	color #8f8f8f
	font-size 1.9rem
	font-family Smedium
	border-bottom 2px solid #e8e8e8
	padding 10px 0
	margin-right 50px
	&:hover
		color #000
		text-decoration none
		border-color #208afe
.faq-nav__item--active
	color #000
	border-bottom 2px solid #208afe
.faq__item
	max-width 566px
	font-size 1.7rem
	line-height 2.5rem
	margin-top 30px
.faq__item-question
	color #2b81d1
	margin 0
	cursor pointer
	transition all .2s
	&:hover
		text-decoration underline
.faq__item--active .faq__item-question
	color #000
.faq__item-answer
	display none
	color #000
	p
		margin 0
.faq__item--active .faq__item-answer
	display block
	animation fade .5s
/*  </faq>  */

/*  <form>  */
.form
	width 410px
	margin 80px auto 0
	a
		color #6298d8
		text-decoration underline
		&:hover
			text-decoration none
	.input-text
		height 47px
.form__title
	color #000
	font-size 3rem
	margin 0 0 40px
.form__row
	margin-bottom 25px
.form__btn
	margin-top 45px
.auth__forgot
	font-size 1.6rem
	text-align center
	margin-top 35px
/*  </form>  */

/*  <remind-password>  */
.remind-complete {
	max-width 438px
	text-align center
	background url('data:image/png;base64,') no-repeat center 0
	padding-top 125px
	margin 124px auto 0
}
.remind-complete__title
	color #000
	font-size 3rem
	margin 0 0 35px
.remind-complete__descr
	color #000
	font-size 1.7rem
	line-height 2.4rem
	p
		margin 0
/*  </remind-password>  */

/*  <news>  */
.news
	overflow hidden
	margin-top 65px
.news__title
	color #000
	font-size 3rem
	margin 0 0 40px
.news__list
	max-width 630px
	margin-top -30px
.news__item
	border-bottom 1px solid #dadada
	padding 45px 0
	&:last-child
		border-bottom 0
		padding 45px 0 0
.news__item-date
	display block
	font-size 1.4rem
	margin-bottom 10px
.news__item-name
	color #000
	font-size 3rem
	margin 0 0 15px
.news__item-pic
	margin-bottom 15px
.news__item-text
	color #000
	font-size 1.7rem
	line-height 2.4rem
	p
		margin 0
.news__detail
	max-width 630px
	margin-top 40px
.news__detail-text
	color #000
	font-size 1.7rem
	line-height 2.4rem
	text-align justify
/*  </news>  */

/*  <for-partners>  */
.for-partners
	max-width 850px
	margin-top 60px
.for-partners__title
	color #000
	font-size 3rem
	margin 0 0 -30px
.for-partners__item
	padding-left 190px
	background-repeat no-repeat
	background-position 0 center
	margin 80px 0
.for-partners__item--referals
	background-image url('../i/for-partners__referal.png')
.for-partners__item--monitoring
	background-image url('../i/for-partners__monitoring.png')
.for-partners__item--widget
	background-image url('../i/for-partners__widget.png')
.for-partners-item__title
	color #000
	font-size 3rem
	line-height 1
	margin 0 0 15px
.for-partners-item__descr
	font-size 1.7rem
	line-height 2.5rem
	p
		margin 0
.for-partners__register
	text-align center
	background-color #e4e4e4
	padding 70px 0 86px
	margin-top 110px
.for-partners__register-title
	color #000
	font-size 3rem
	margin 0 0 25px
.for-partners__btn
	padding 0 46px
/*  </for-partners>  */

/*  <rules>  */
.rules {
	max-width 988px
	margin-top 70px
}
.rules__title
	color #000
	font-size 3rem
	margin 0 0 30px
.rules__block
	font-size 1.7rem
	line-height 2.7rem
	margin 30px 0
	&:last-of-type
		margin 30px 0 0
	p
		margin 0
.rules-block__title
	color #202020
/*  </rules>  */