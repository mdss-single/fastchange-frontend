	&--referal
		padding-left 31px
		&:before
			width 19px
			height 23px
			background-image url("data:image/png;base64,")
		&^[0]--active
		&:hover
			&:before
				background-image url("data:image/png;base64,")
	&--promo
		padding-left 31px
		&:before
			width 19px
			height 19px
			background-image url("data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABMAAAATBAMAAACAfiv/AAAAIVBMVEUAAAChvtmhvtmhvtmhvtmhvtmhvtmhvtmhvtmhvtmhvtnVYrZVAAAACnRSTlMA369I17fF5/PLz1LRegAAAEVJREFUCNdjSBSEggQGcSUlpawkJSV1AQYhBgYGKwMGBiYymCIuLh5Q5tTQ0HAUBeKhoaGzIEwJFxcXKRCTbNsQ7kXyBQCZAhKZX6QZeAAAAABJRU5ErkJggg==")
		&^[0]--active
		&:hover
			&:before
				background-image url("data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABMAAAATBAMAAACAfiv/AAAAJFBMVEUAAAD///////////////////////////////////////////+0CY3pAAAAC3RSTlMA369I17fF5vPpy3GlHwMAAABFSURBVAjXY0gUhIIEBnElJaXsJCUldQEGIQYGBmsDBgYmMpgiLi4eUObS0NBIFAWSoaGhqyFMCRcXF2kQk2zbEO5F8gUArq8Szuo26WMAAAAASUVORK5CYII=")
